{
    "name": "jaspaul/eloquent-model-validation",
    "description": "A simple package that provides a venue for model validation in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.40",
        "illuminate/validation": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "mockery/mockery": "^1.4.1",
        "squizlabs/php_codesniffer": "^3.1",
        "php-coveralls/php-coveralls": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Jaspaul\\EloquentModelValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
